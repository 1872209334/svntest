<ion-header>

  <ion-navbar>
    <ion-title></ion-title>
  </ion-navbar>

</ion-header>

<ion-content padding>
  <div style="margin-left:16px;margin-top:30px;width:100%;font-size:32px;font-weight: bold" > 设置新密码 <br/>
    <span style="margin-left:0px;margin-top:100px;width:100%;font-size:16px;color:#909399" > 请设置登录密码 </span>
  </div>



  <ion-item style="margin-left:0px;margin-top:24px;">
    <ion-label style="color:black;width:22%;font-size:18px;">密码</ion-label>
    <ion-input style="color:darkblue;width:60%;font-size:18px;" [(ngModel)]="registerInput.password" type="text" placeholder="请设置密码"></ion-input>
  </ion-item>


  <button  ion-button class="login_btn" round style="margin-top: 4rem"  (click)="setPassword()">
    确定
  </button>

</ion-content>
