page-search {
  .content{
    background: #F5F7FA;
  }

  .search_header{
    width: 100%;
    height: 4.5rem;
    background: #fff;
  }
  .project_select_item,.item-divider-ios{
    margin: 0;
    padding: 0;
    background: #fff;
  }
  .search_part{
    width: 100%;
    display: inline-block;
    min-height:100%;
    background-color: #F5F7FA;
    padding:1rem;
  }
  .devicerow{
    box-shadow: 0 0 0 !important;
    background-color: #fff;
  }
  .project_item_text{
    font-size: 1.6rem;
    height: 3rem;
  }
  .project_item_text p{
    display: inline-block;
    vertical-align: top;
  }
  .project_item_img{
    width: 3rem;
    display: inline-block;
    float: right;
    position: relative;
    top: -0.5rem;
  }

  .project_item_title{
    color: #303133;
    font-size: 2rem;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    width: 100%;
    position: absolute;
  }

  .project_address{
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    width: 100%;
    position: absolute;
    text-align: right;
    padding-right: 1rem;
  }

  .project_item_color1{
    color: #387EE8;
  }

  .project_item_color2{
    color: #909399;
  }

  .project_item_color3{
    color: #F56C6C;
  }

  .project_float_right{
    float: right;
  }
  .project_margin{
    margin-top: 0.3rem;
  }
}
