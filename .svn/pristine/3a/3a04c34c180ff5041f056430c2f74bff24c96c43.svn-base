page-info1 {
  .me_background{
    background-image: url('../../assets/imgs/avatar.png');
    background-position: center;
    background-size: cover;
    width: 100%;
    height:8rem;
  }

  .me-item-start{
  margin: 8px 16px 8px 0; }

  .item-ios.item-block .item-inner{
    border: none;
  }

  .header-ios .toolbar-background-ios{
    border: none;
  }

  .info_box{

  }
  .info_header_pictrue{
    width: 5rem;
    height: 5rem;
    border-radius: 5rem;
    margin-right: 0.5rem;
  }

  .info_name{
    display: inline-block;
    width: calc(100% - 5rem);
    vertical-align: top;
    height: 5rem;
    line-height: 3rem;
  }

  .user_phone_box{
    position: relative;
    margin-top: 1.5rem;
  }

  .user_info_phone_num{
    position: absolute;
    right: 4rem;
    color: #75561B;
    font-size: 16px;
    top: 20px;
  }

  .user_phone_num_first{
    color: #909399;
    font-size: 1.6rem;
    vertical-align: middle;
  }

  .label-ios{
    margin-bottom: 0;
  }

  .user_name_text{
    color: #303133;
  }

  .info_phone_icon{
    width: 1.1rem;
  }

  .info_item_icon{
    width: 2rem;
    display: inline-block;
    vertical-align: middle;
    margin-right: 1rem;
  }

  .info_item_text{
    color: #303133;
  }

  .info_item_v{
    color: #909399;
    float: right;
  }

  .user_item_button{
    padding-left: 16px !important;
  }

  .info_item{
    border-bottom: 1px solid #CDD0D8;
    padding-right: 16px;
  }
  .info_item .label{
    margin-right: 0;
    margin-bottom: 11px;
  }
}
