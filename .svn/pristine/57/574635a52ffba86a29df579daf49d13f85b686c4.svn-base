page-alarm {
  .content {
    background-color: #F5F7FA;
  }
  .history_common_text{
    font-size: 1.4rem;
    color: #909399;
  }
  .history_common_text2{
    font-size: 1.5rem;
    color:#387EE8;
  }
  .history_common_text3{
    font-size: 1.5rem;
    color: #303133;
  }
  .history_common_space{
    white-space: normal;
    overflow:hidden;
    text-overflow:ellipsis;
    display:-webkit-box;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:2;
  }
  .list_box{
    /*  background-image: url('../../assets/imgs/login_bg.png');*/
    background-position: center;
    background-size: cover;
    background: #fff;
    padding-bottom: 1rem;
  }
  .history_title{
    font-size: 2rem;
    color: #303133;
  }
  .history_header{
    height: 4.5rem;
    border-bottom: 1px solid #CDD0D8;
    text-align: center;
    line-height: 4.5rem;
  }
  .list_title{
    width: 100%;
    height:4rem;
    top: 9rem;
    text-align: center;
    position: absolute;
    font-size: 20px;
    color: #ffffff;
  }
  .select-icon-inner{
    /* color: #fff !important;*/
  }
  .select-text{
    color:#387EE8;
    font-size: 1.6rem;
  }
  .device_box_part{
    width: 50%;
    position: absolute;
    top: 1rem;
    height: 5rem;
    font-size: 17px;
  }
  .device_box_part p{
    width:100%;
    text-align:center;
    color:#ffffff;
  }
  .select-icon{
  }

  .history_time{
    background-color: #f7f7f7;
    margin: 0 20px;
    border-radius: 2px;
  }

  .history_time_row{
    color:#387EE8;
    width: 100%;
    height:6rem;
    position: relative;
    top: 1rem;
    display: flex;
  }

  .history_time_row label{
    color:#909399;
    font-size: 1.6rem;
    text-align: center;
    margin-left: 1rem;
  }

  .history_time_left{
    width: 50%;
    padding-left: calc(25% - 3.2rem);
  }

  .history_time_right{
    width: 50%;
    padding-left: calc(25% - 3.2rem);
  }

  .history_time_center{
    position: absolute;
    left: calc(50% - 1.6rem);
    top: 1rem;
  }

  .text-input-ios{
    margin: 0;
  }

  .history_select{
    max-width: 100%;
  }

  .history_list{
    margin: 1rem;
  }

  .history_bian{
    width: 1.6rem;
    height:1.6rem;
    border-radius: 1px;
    background-color: #F5A50B;
    color:#fff;
    display: inline-block;
    text-align: center;
    line-height: 1.6rem;
    font-size: 10px;
  }
  .history_img{
    width: 1rem;
  }
  .history_float_left,.history_float_right{
    width: 50%;
  /*  display: inline-block;*/
    vertical-align: middle;
  }
  .history_float_right{
  /*  float:right*/
  }

  .do_warm{
    display: inline-block;
    float: right;
    border-radius: 20px;
    padding: 0.8rem 1.4rem;
    background: #387EE8;
    color: #fff;
    font-size: 14px;
  }
}
