page-map {
  .map_common_text{
    font-size: 1.4rem;
    line-height: 1.6rem;
    letter-spacing: 2px;
  }
  .map_text_color1{
    color:#909399;
  }
  .map_text_color2{
    color:#387EE8;
  }
  .map_text_color3{
    color:#F56C6C;
  }
  .map_text_color4{
    color:#75561B;
  }
  .map_msg1{
    width: calc(50% - 1rem);
    display: inline-block;
    margin-bottom: 0.5rem;
  }
  .map_msg2{
    display: flex;
    margin-bottom: 0.5rem;
  }
  .map_msg2>span:last-child{
    flex: 1;
  }
  .map_box_bg1{
    margin-top:0.5rem;
    margin-bottom: 0.5rem;
    background: url("../../assets/imgs/map_img_admin_1.png");
    background-repeat: round;
    width:100%;
    height: 54px;
    text-align: right;
    line-height: 54px;
    padding-right: 4rem;
  }
  .map_box{
    width:100%;
    position: absolute;
    padding: 1rem 1.5rem;
    z-index: 100;
    min-height:15rem;
    top: 0;
  }
  .map_msg_box{
    width: 100%;
    height: 100%;
    background:#fff;
    border-radius: 3px;
    box-shadow:0 1px 4px rgba(0, 0, 0, 0.16);
    padding: 1rem 1.5rem;
  }

  .map_item_header{
    margin: 0.5rem 0;
  }

  .mapBoxHide{
    display: none;
  }

  .map_tel{
    position: relative;
    top: -3.1rem;
    float: right;
    right: 4rem;
    color: #75561B;
    font-size: 1.6rem;
  }
}
