<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:fitsSystemWindows="true"
    android:gravity="center"
    android:orientation="vertical" >

         <RelativeLayout
             android:layout_width="match_parent"
             android:layout_height="50dp"
             android:background="#5f9fff" >
	
	        <Button
	            android:id="@+id/return_btn"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_alignParentLeft="true"
	            android:layout_centerVertical="true"
	            android:layout_marginLeft="8dp"
	            android:background="@null"
	            android:paddingBottom="5dp"
	            android:paddingLeft="15dp"
	            android:paddingRight="10dp"
	            android:paddingTop="5dp"
	            android:text="返回"
	            android:textColor="#ffffff" />
	
	        <TextView
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
				android:layout_centerHorizontal="true"
				android:layout_centerVertical="true"
				android:layout_centerInParent="true"
	            android:text="地图"
	            android:textColor="#ffffff"
	            android:textSize="19sp"
	            android:textStyle="bold" />

	            <!--   <Button
	                  android:id="@+id/ljdh_btn"
	                  android:layout_width="wrap_content"
	                  android:layout_height="wrap_content"
	                  android:layout_alignParentRight="true"
	                  android:layout_centerVertical="true"
	                  android:layout_marginRight="8dp"
	                  android:background="@null"
	                  android:paddingBottom="5dp"
	                  android:paddingLeft="10dp"
	                  android:paddingRight="10dp"
	                  android:paddingTop="5dp"
	                  android:text="列表"
	                  android:textColor="#ffffff" /> -->
	
	    </RelativeLayout>


 		<RelativeLayout
 		    android:layout_width="wrap_content"
 		    android:layout_height="fill_parent"
 		    android:orientation="vertical" >

 		    <com.baidu.mapapi.map.MapView
 		        android:id="@+id/mapsView"
 		        android:layout_width="fill_parent"
 		        android:layout_height="fill_parent"
 		        android:clickable="true" />


 		    <Button
 		        android:id="@+id/dingwei"
 		        style="@style/AppTheme"
 		        android:layout_width="40dp"
 		        android:layout_height="40dp"
 		        android:layout_alignParentBottom="true"
 		        android:layout_alignParentLeft="true"
 		        android:layout_marginBottom="120dp"
 		        android:layout_marginLeft="10.0dip" 
 		        android:background="@drawable/custom_loc" />
			 <LinearLayout
 		         android:id="@+id/linearLayout_pl"
 		         android:layout_width="match_parent"
       			 android:layout_height="wrap_content"
 		        android:layout_alignParentBottom="true"
 		        android:layout_alignParentLeft="true"
 		        android:layout_alignParentRight="true"
 		        android:layout_marginBottom="50dp" 
 		        android:layout_marginRight="10.0dip" 
 		        android:layout_marginLeft="10.0dip" 
 		        android:paddingLeft="10.0dip"
        		android:paddingRight="10.0dip"
 		        android:background="@drawable/btn_white_selector"
 		        android:visibility="invisible"
 		        >
 		        <TextView
            android:id="@+id/tv_code_lps"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="3dp"
            android:padding="10dp"
            android:textColor="@color/dark"
            android:textSize="16sp" 
           />

        <TextView
            android:id="@+id/tv_pointcategory_lps"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:gravity="right"
            android:textColor="@color/grey"
            android:textSize="13sp" />
        
 		</LinearLayout>
 			
 		    <LinearLayout
 		        android:id="@+id/linearLayout_lp"
 		        android:layout_width="wrap_content"
 		        android:layout_height="wrap_content"
 		        android:layout_alignParentBottom="true"
 		        android:layout_alignParentLeft="true"
 		        android:layout_alignParentRight="true"
 		        android:layout_marginBottom="21dp"
 		        android:paddingLeft="10.0dip"
      			android:paddingRight="10.0dip"
 		        android:visibility="invisible" 
 		        >

 		     <TextView
	            android:id="@+id/tv_detail_lps"
	            android:layout_width="0.0dip"
	            android:layout_height="fill_parent"
	            android:layout_weight="1.0"
	            android:background="@drawable/btn_white_selector"
	            android:clickable="true"
	            android:gravity="center"
	            android:text="读取"
	            android:textColor="@drawable/btn_white_textcolor_selector" />
			<ImageView
	            android:layout_width="1dp"
	            android:layout_height="fill_parent"
	            android:layout_marginBottom="15dp"
	            android:layout_marginLeft="-1.0px"
	            android:layout_marginTop="15dp"
	            android:background="@drawable/ic_line_grey" />
 		    <TextView
	            android:id="@+id/tv_nevigation_lps"
	            android:layout_width="0.0dip"
	            android:layout_height="fill_parent"
	            android:layout_weight="1.0"
	            android:background="@drawable/btn_white_selector"
	            android:clickable="true"
	            android:gravity="center"
	            android:paddingLeft="5dp"
	            android:text="设置"
	            android:textColor="@drawable/btn_white_textcolor_selector" />
	         <ImageView
	            android:layout_width="1dp"
	            android:layout_height="fill_parent"
	            android:layout_marginBottom="15dp"
	            android:layout_marginLeft="-1.0px"
	            android:layout_marginTop="15dp"
	            android:background="@drawable/ic_line_grey" />
 		    <TextView
	            android:id="@+id/tv_previous_lps"
	            android:layout_width="0.0dip"
	            android:layout_height="fill_parent"
	            android:layout_weight="1.0"
	            android:background="@drawable/btn_white_selector"
	            android:clickable="true"
	            android:gravity="center"
	            android:paddingLeft="5dp"
	            android:text="上一个"
	            android:textColor="@drawable/btn_white_textcolor_selector" />
 		         <ImageView
		            android:layout_width="1dp"
		            android:layout_height="fill_parent"
		            android:layout_marginBottom="15dp"
		            android:layout_marginLeft="-1.0px"
		            android:layout_marginTop="15dp"
		            android:background="@drawable/ic_line_grey" />
 		     <TextView
	            android:id="@+id/tv_next_lps"
	            android:layout_width="0.0dip"
	            android:layout_height="fill_parent"
	            android:layout_weight="1.0"
	            android:background="@drawable/btn_white_selector"
	            android:clickable="true"
	            android:gravity="center"
	            android:paddingLeft="5dp"
	            android:text="下一个"
	            android:textColor="@drawable/btn_white_textcolor_selector" />

 		    </LinearLayout>

 		</RelativeLayout>

</LinearLayout>